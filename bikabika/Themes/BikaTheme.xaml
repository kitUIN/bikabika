                        x:Key="SystemAccentAcrylicColorDark3"
                        BackgroundSource="Backdrop"
                        FallbackColor="{ThemeResource SystemAccentColorDark3}"
                        TintColor="{ThemeResource SystemAccentColorDark3}"
                        TintOpacity="0.8" />
                    <AcrylicBrush
                        x:Key="SystemAccentAcrylicColorDark2"
                        BackgroundSource="Backdrop"
                        FallbackColor="{ThemeResource SystemAccentColorDark2}"
                        TintColor="{ThemeResource SystemAccentColorDark2}"
                        TintOpacity="0.8" />
                    <AcrylicBrush
                        x:Key="SystemAccentAcrylicColorDark1"
                        BackgroundSource="Backdrop"
                        FallbackColor="{ThemeResource SystemAccentColorDark1}"
                        TintColor="{ThemeResource SystemAccentColorDark1}"
                        TintOpacity="0.8" />
                    <AcrylicBrush
                        x:Key="SystemAccentAcrylicColorLight5"
                        BackgroundSource="Backdrop"
                        FallbackColor="{ThemeResource SystemAccentColorLight5}"
                        TintColor="{ThemeResource SystemAccentColorLight5}"
                        TintOpacity="0.8" />
                    <AcrylicBrush
                        x:Key="SystemAccentAcrylicColorLight4"
                        BackgroundSource="Backdrop"
                        FallbackColor="{ThemeResource SystemAccentColorLight4}"
                        TintColor="{ThemeResource SystemAccentColorLight4}"
                        TintOpacity="0.8" />
                    <AcrylicBrush
                        x:Key="SystemAccentAcrylicColorLight3"
                        BackgroundSource="Backdrop"
                        FallbackColor="{ThemeResource SystemAccentColorLight3}"
                        TintColor="{ThemeResource SystemAccentColorLight3}"
                        TintOpacity="0.8" />
                    <AcrylicBrush
                        x:Key="SystemAccentAcrylicColorLight2"
                        BackgroundSource="Backdrop"
                        FallbackColor="{ThemeResource SystemAccentColorLight2}"
                        TintColor="{ThemeResource SystemAccentColorLight2}"
                        TintOpacity="0.8" />
                    <AcrylicBrush
                        x:Key="SystemAccentAcrylicColorLight1"
                        BackgroundSource="Backdrop"
                        FallbackColor="{ThemeResource SystemAccentColorLight1}"
                        TintColor="{ThemeResource SystemAccentColorLight1}"
                        TintOpacity="0.8" />
                    <AcrylicBrush
                        x:Key="SystemAccentAcrylicColor"
                        BackgroundSource="Backdrop"
                        FallbackColor="{ThemeResource SystemAccentColor}"
                        TintColor="{ThemeResource SystemAccentColor}"
                        TintOpacity="0.8" />
                    <Color x:Key="RegionColor">#FF000000</Color>
                    <SolidColorBrush x:Key="RegionBrush" Color="{StaticResource RegionColor}" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <ResourceDictionary.MergedDictionaries>
                <ColorPaletteResources
                    Accent="#FFFA97BB"
                    AltHigh="#FFFFFFFF"
                    AltLow="#FFFFFFFF"
                    AltMedium="#FFFFFFFF"
                    AltMediumHigh="#FFFFFFFF"
                    AltMediumLow="#FFFFFFFF"
                    BaseHigh="#FF000000"
                    BaseLow="#FFCCCCCC"
                    BaseMedium="#FF898989"
                    BaseMediumHigh="#FF5D5D5D"
                    BaseMediumLow="#FF737373"
                    ChromeAltLow="#FF5D5D5D"
