                <ColorPaletteResources
                    Accent="#FFFA97BB"
                    AltHigh="#FF000000"
                    AltLow="#FF000000"
                    AltMedium="#FF000000"
                    AltMediumHigh="#FF000000"
                    AltMediumLow="#FF000000"
                    BaseHigh="#FFFFFFFF"
                    BaseLow="#FF333333"
                    BaseMedium="#FF9A9A9A"
                    BaseMediumHigh="#FFB4B4B4"
                    BaseMediumLow="#FF676767"
                    ChromeAltLow="#FFB4B4B4"
