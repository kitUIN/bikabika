                        TintOpacity="0.8" />
                    <AcrylicBrush
                        x:Key="SystemAccentAcrylicColorLight4"
                        BackgroundSource="Backdrop"
                        FallbackColor="{ThemeResource SystemAccentColorLight4}"
                        TintColor="{ThemeResource SystemAccentColorLight4}"
                        TintOpacity="0.8" />
                    <AcrylicBrush
                        x:Key="SystemAccentAcrylicColorLight3"
                        BackgroundSource="Backdrop"
                        FallbackColor="{ThemeResource SystemAccentColorLight3}"
                        TintColor="{ThemeResource SystemAccentColorLight3}"
                        TintOpacity="0.8" />
                    <AcrylicBrush
