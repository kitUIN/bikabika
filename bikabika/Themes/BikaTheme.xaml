                    <Color x:Key="SystemAccentColorLight5">#FFFFD5E8</Color>
                    <Color x:Key="SystemAccentColorDark1">#FFE480A7</Color>
                    <Color x:Key="SystemAccentColorDark2">#FFCF6993</Color>
                    <Color x:Key="SystemAccentColorDark3">#FFB9527E</Color>
                    <Color x:Key="SystemAccentColorDark4">#FFA43B6A</Color>
                    <Color x:Key="SystemAccentColorDark5">#FF8E2456</Color>
                    <AcrylicBrush
                        x:Key="SystemAccentAcrylicColorDark5"
                        BackgroundSource="Backdrop"
                        FallbackColor="{ThemeResource SystemAccentColorDark5}"
